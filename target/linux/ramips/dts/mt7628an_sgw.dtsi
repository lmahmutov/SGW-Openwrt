#include "mt7628an.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "sgw,homed-32m", "mediatek,mt7628an-soc";

	aliases {
		led-boot = &led_wifi;
		led-failsafe = &led_wifi;
		led-running = &led_wifi;
		led-upgrade = &led_wifi;
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	leds {
		compatible = "gpio-leds";

		led_wifi: wifi {
			label = "orange:wifi";
			gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};


};

&state_default {
	gpio {
		groups = "gpio";
		function = "gpio";
	};

	perst {
		groups = "perst";
		function = "gpio";
	};

	refclk {
		groups = "refclk";
		function = "gpio";
	};

	i2s {
		groups = "i2s";
		function = "gpio";
	};

	spis {
		groups = "spis";
		function = "gpio";
	};

	wled_kn {
		groups = "wled_kn";
		function = "gpio";
	};

	wled_an {
		groups = "wled_an";
		function = "wled_an";
	};

	wdt {
		groups = "wdt";
		function = "gpio";
	};
};

&i2c {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&pwm {
	status = "okay";
};

&ethernet {
	mtd-mac-address = <&factory 0x28>;
};

&sdhci {
	status = "okay";
	mediatek,cd-low;
};

&wmac {
	status = "okay";
};
